{
  "path-intellisense.mappings": {
    "/static/": "${workspaceFolder}/public/static/"
  },
  "editor.rulers": [
    {
      "column": 76,
      "color": "rgba(255, 255, 0, .3)"
    },
    {
      "column": 80,
      "color": "rgba(255, 255, 0, .3)"
    },
    {
      "column": 120,
      "color": "rgba(255, 255, 255, .3)"
    }
  ],
  "yaml.schemas": {
    "https://ci.coding.net/docs/conf-schema.json": ".coding-ci.yml"
  }
}
